<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cobrain.android"
    android:versionCode="14"
    android:versionName="1.0.2" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />
    
    <uses-permission android:name="android.permission.INTERNET" />
	<uses-permission android:name="android.permission.SEND_SMS"/>
	<uses-permission android:name="android.permission.READ_CONTACTS"/>
	<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />	
	
    <application
		android:screenOrientation="portrait"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.Cobrain" >
        <activity
            android:name="com.cobrain.android.MainActivity"
			android:screenOrientation="portrait"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
			<intent-filter>
				<action android:name="android.intent.action.VIEW" />
   				<category android:name="android.intent.category.DEFAULT" />
				<category android:name="android.intent.category.BROWSABLE" />
				<data android:host="www.cobrain.com" android:scheme="http" />
				<data android:host="www.cobrain.com" android:scheme="https" />
				<data android:host="cobrain.com" android:scheme="http" />
				<data android:host="cobrain.com" android:scheme="https" />
				<data android:host="qa.cobrain.com" android:scheme="http" />
				<data android:host="qa.cobrain.com" android:scheme="https" />
			</intent-filter>
        </activity>
    </application>

</manifest>
